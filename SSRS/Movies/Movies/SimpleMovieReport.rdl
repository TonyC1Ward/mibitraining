  <DataSets>
    <DataSet Name="ds_MovieList">
      <Query>
        <DataSourceName>Movies</DataSourceName>
        <CommandText>SELECT   tblDirector.DirectorName AS Director, tblStudio.StudioName AS Studio, tblCertificate.CertificateName AS Certificate, tblFilm.FilmName AS Title, tblFilm.FilmReleaseDate AS ReleaseDate, tblFilm.FilmOscarWins AS OscarWins
FROM      tblCertificate INNER JOIN
                tblFilm ON tblCertificate.CertificateID = tblFilm.FilmCertificateID INNER JOIN
                tblDirector ON tblFilm.FilmDirectorID = tblDirector.DirectorID INNER JOIN
                tblStudio ON tblFilm.FilmStudioID = tblStudio.StudioID</CommandText>
      </Query>
      <Fields>
        <Field Name="Director">
          <DataField>Director</DataField>
          <rd:TypeName>System.String</rd:TypeName>
